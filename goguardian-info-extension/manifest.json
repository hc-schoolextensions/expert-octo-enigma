{
  "manifest_version": 3,
  "name": "GoGuardian Blocked Webpage Info",
  "description": "Adds a button to the GoGuardian blocked page to give extra information about what and why a website was blocked.",
  "author": "Hayden C",
  "version": "0.1",
  "icons": { "128": "icon128.png" },
  "content_scripts": [
    {
      "matches": ["*://*.blocked.goguardian.com/*"],
      "css": ["goguardian.css"],
      "js": ["goguardian.js"],
      "all_frames": true
    }
  ],
  "key": ""
}
